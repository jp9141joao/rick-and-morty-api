generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tb_usuario {
  id    BigInt @id(map: "pk_tb_usuario_id") @default(autoincrement())
  nome  String @db.VarChar(30)
  email String @unique @db.VarChar(255)
  senha String @db.VarChar(255)
}
